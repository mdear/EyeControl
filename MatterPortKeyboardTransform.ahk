#MaxHotkeysPerInterval 1000000 ; Avoid popup
0::J       ; Look left
1::L       ; Look right
2::I       ; Look up
3::K       ; Look down
4::A       ; Walk left
5::D       ; Walk right
6::W       ; Walk forward
7::S       ; Walk backward
8::R       ; Rise up
9::F       ; Sink down
